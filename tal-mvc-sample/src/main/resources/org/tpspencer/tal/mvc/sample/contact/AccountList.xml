<?xml version="1.0"?>

<!--
 | AccountList.xml
 | Overrides for the list of accounts 
 -->

<templates>

<template templateClass="org.tpspencer.tal.mvc.sample.model.account.Account" autoDiscover="false">
	<image-group name="icon" resource="resource/org/tpspencer/tal/mvc/sample/contact/account.png" />
	<action-group name="account" action="selectAccount" params="id=${this.id}">
		<string-prop name="id" />
		<string-prop name="company" />
	</action-group>
</template>

</templates>