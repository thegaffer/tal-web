<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="
			http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
			http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-2.5.xsd">
	
	<import resource="classpath:/org/tpspencer/tal/mvc/sample/objex/sample-context-overrides.xml"/>
	
	<!-- Don't like this being GAE in test -->
	<bean id="objexMiddlewareFactory" class="org.tpspencer.tal.objexj.gae.GAEMiddlewareFactory" />
			
	<bean id="accountRepository" class="org.tpspencer.tal.mvc.commons.repository.PrototypeRepository">
		<property name="objects"><map>
			<entry key="123456"><bean p:id="123456" p:company="Test Inc" class="org.tpspencer.tal.mvc.sample.model.account.AccountBean" /></entry>
		</map></property>
	</bean>
	
</beans>